{
  "name": "webpack-no-dependencies-boilerplate",
  "version": "1.0.0",
  "description": "Simple webpack boilerplate using only dev dependencies for beginners",
  "main": "main.js",
  "browserslist": "last 3 versions, ie >= 10, not dead",
  "scripts": {
    "dev": "node --no-deprecation build/server.js",
    "build": "node --no-deprecation build/build.js",
    "test": "node --no-deprecation ./node_modules/karma/bin/karma start build/config/karma.config.js --single-run",
    "watch": "node --no-deprecation ./node_modules/karma/bin/karma start build/config/karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokernakartama/webpack-no-dependencies-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "simple",
    "no dependencies",
    "boilerplate"
  ],
  "author": "Sergey Kobzev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jokernakartama/webpack-no-dependencies-boilerplate/issues"
  },
  "homepage": "https://github.com/jokernakartama/webpack-no-dependencies-boilerplate#README",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "import-glob-loader": "^1.1.0",
    "karma": "^4.4.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.0.0",
    "node-sass": "^4.13.0",
    "ora": "^4.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.1",
    "sinon": "^8.0.4",
    "sinon-chai": "^3.4.0",
    "standard": "^14.3.1",
    "style-loader": "^1.1.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.3.2",
    "webpack": "^4.41.5",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
